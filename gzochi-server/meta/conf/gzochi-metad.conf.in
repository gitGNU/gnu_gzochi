# 
# A minimal version of gzochi-metad.conf, the gzochi meta server configuration
# file; intended for use running gzochi-metad from the build tree.
#

[admin]

context.enabled = true
module.httpd.enabled = true

[data]

server.fs.data = @abs_top_builddir@/meta/data
storage.engine.dir = @abs_top_builddir@/src/storage
